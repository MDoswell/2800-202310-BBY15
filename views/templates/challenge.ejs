<h5 style="color:rgb(15, 99, 172)"><%= exercise.exerciseName%></h5>
    
        <fieldset id="<%=group%>">
            <p>How challenging was this exercise?</p>
            <div class="btn-group" role="group" aria-label="Basic radio toggle button group">
        
        <input type="radio" class="btn-check" name="<%=group%>" id="form<%= exercise.exerciseName%>1" autocomplete="off">
        <label class="btn btn-outline-primary" for="form<%= exercise.exerciseName%>1">Not a challenge</label>
      
